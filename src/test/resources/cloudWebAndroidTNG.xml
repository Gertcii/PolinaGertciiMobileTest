<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="EPAM mobile cloud Android Web test suite">
    <parameter name="appType" value="web"></parameter>
    <parameter name="deviceName" value="Google Pixel 2 XL"></parameter>
    <!--parameter name="udid" value="XPH0219920001076"></parameter-->
    <parameter name="platformName" value="Android"></parameter>
    <parameter name="browserName" value="Chrome"></parameter>
    <test name="Cloud android web test">
        <groups>
            <run>
                <include name="web"/>
            </run>
        </groups>
        <classes>
            <class name="scenarios.WebMobileTests"></class>
        </classes>
    </test>
</suite>